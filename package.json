{
  "name": "faceframe",
  "version": "0.1.0",
  "description": "Local face detection and photo organization app",
  "private": true,
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none npm run dev\" \"wait-on http://localhost:1420 && cross-env ELECTRON_START_URL=http://localhost:1420 electron electron/main.cjs\"",
    "electron:build": "npm run build && electron-builder"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "wait-on": "^9.0.3"
  },
  "build": {
    "appId": "com.faceframe.app",
    "productName": "FaceFrame",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "python-backend/**/*"
    ],
    "win": {
      "target": "nsis"
    }
  }
}